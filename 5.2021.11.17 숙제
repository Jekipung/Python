x=0
y=0
a=[]
c=1
while c < 100:
    c=c+1
    if y!=9:
        y=y+1
    elif y==9:
        y=0
        x=x+1
    if x!=0 and y!=0 and x%3==0 and y%3==0:
        a.append("**")
    elif (x!=0 and x%3==0) or (y!=0 and y%3==0):
        a.append("*")
    else:
        a.append(10*x+y)
print(a)

[주석]
x=0
#X는 10의자리 숫자.
y=0
#Y는 1의자리 숫자.
a=[]
#리스트 a.
c=1
#C 100번 반복을 위한 변수.
while c < 100:
    #c가 100보다 작을경우 반복
    c=c+1
    #C에 1을 더해서 100번을 반복한다.
    if y!=9:
        y=y+1
        #1의 자리 숫자인 Y가 9가 아닐경우 1을 더한다.
    elif y==9:
        y=0
        x=x+1
        #1의 자리 숫자인 Y가 9일경우 Y를 0으로 바꾸고 10의자리 숫자인 Y에 1을 더한다.
    if x!=0 and y!=0 and x%3==0 and y%3==0:
        a.append("**")
        #10의자리 숫자와 1의자리 숫자가 0이 아니면서 3,6,9중 하나일 경우 **
    elif (x!=0 and x%3==0) or (y!=0 and y%3==0):
        a.append("*")
        #10의자리 수가 0이 아니면서 10의자리 수가 3,6,9중 하나 이거나 1의자리 숫자가 0이 아니고 3,6,9중 하나일 경우 *
    else:
        a.append(10*x+y)
        #아닐경우 그냥 숫자
print(a)
#프린트

[최적화]
a=[]
c=0
while c < 100:
    c=c+1
    y = c % 10
    x = c // 10
    if x!=0 and y!=0 and x%3==0 and y%3==0:
        a.append("**")
    elif (x!=0 and x%3==0) or (y!=0 and y%3==0):
        a.append("*")
    else:
        a.append(10*x+y)
print(a)
